<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="ReactiveRLA"/>
		<set discount-factor="0.85"/>
	</agent>
	<agent name="greedy-bot">
		<set class-path="bin/"/>
		<set class-name="GreedyBot"/>
		<set discount-factor="0.85"/>
	</agent>
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="ReactiveTemplate" />
		<set discount-factor="0.85"/>
	</agent>


</agents>
